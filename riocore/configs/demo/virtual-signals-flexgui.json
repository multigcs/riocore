{
    "name": "Tangbob",
    "boardcfg": "Tangbob",
    "description": "Virtual-Signals with Axis and GladeVCP",
    "protocol": "UDP",
    "plugins": [
        {
            "type": "w5500",
            "pins": {
                "mosi": {
                    "pin": "SPI:MOSI"
                },
                "miso": {
                    "pin": "SPI:MISO"
                },
                "sclk": {
                    "pin": "SPI:SCLK"
                },
                "sel": {
                    "pin": "SPI:SEL"
                }
            },
            "uid": "w55000"
        },
        {
            "type": "wled",
            "pins": {
                "data": {
                    "pin": "WLED:DATA"
                }
            },
            "signals": {
                "0_green": {
                    "_net": "(!halui.mode.is-auto and !axisui.error) or halui.program.is-paused"
                },
                "0_red": {
                    "_net": "axisui.error"
                },
                "0_blue": {
                    "net": "halui.mode.is-auto"
                }
            },
            "uid": "wled0"
        },
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "10"
                }
            },
            "uid": "blink0"
        },
        {
            "type": "signal",
            "uid": "signal0",
            "signals": {
                "value": {
                    "display": {
                        "section": "Status",
                        "type": "scale",
                        "min": 0.0,
                        "max": 100.0,
                        "initval": 50.0
                    }
                }
            },
            "name": "Virt-Float-In1"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "net": "riov.Virt-Float-In1.value <= 50",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": "<= 50"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Under_Equal"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "net": "riov.Virt-Float-In1.value >= 50",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": ">= 50"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Over_Equal"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "net": "riov.Virt-Float-In1.value < 50",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": "< 50"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Under"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "net": "riov.Virt-Float-In1.value > 50",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": "> 50"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Over"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "net": "riov.Virt-Float-In1.value <> 40,60",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": "between 40-60"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Inside"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "net": "!(riov.Virt-Float-In1.value <> 40,60)",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": "outside 40-60"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Outside"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "_net": "riov.Virt-Float-In1.value >= sig_riov_Virt-Float-In2_value",
                    "net": "riov.Virt-Float-In1.value >= riov.Virt-Float-In2.value",
                    "display": {
                        "section": "Status",
                        "color": "green",
                        "off_color": "red",
                        "title": "In1 >= In2"
                    }
                }
            },
            "vtype": "bool",
            "dir": "output",
            "name": "Val1_Over_Val2"
        },
        {
            "type": "signal",
            "uid": "signal0",
            "signals": {
                "value": {
                    "display": {
                        "section": "Status",
                        "type": "scale",
                        "min": 0.0,
                        "max": 100.0,
                        "initval": 0.0
                    }
                }
            },
            "name": "Virt-Float-In2"
        },
        {
            "type": "signal",
            "uid": "signal1",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "bar",
                        "min": 0.0,
                        "max": 100.0
                    },
                    "net": "riov.Virt-Float-In1.value"
                }
            },
            "dir": "output",
            "name": "Float-Out1"
        },
        {
            "type": "signal",
            "uid": "signal1",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "bar",
                        "min": 0.0,
                        "max": 100.0
                    },
                    "net": "limit(riov.Virt-Float-In1.value, 25.0, 75.0)"
                }
            },
            "dir": "output",
            "name": "Limit"
        },
        {
            "type": "signal",
            "uid": "signal1",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "bar",
                        "min": 0.0,
                        "max": 100.0
                    },
                    "net": "deadzone(riov.Virt-Float-In1.value, 50.0, 10.0)"
                }
            },
            "dir": "output",
            "name": "Deadzone"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "title": "1 + 2",
                        "type": "meter",
                        "min": 0.0,
                        "max": 200.0
                    },
                    "net": "riov.Virt-Float-In1.value + riov.Virt-Float-In2.value"
                }
            },
            "dir": "output",
            "name": "Virt-Float-Out2"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "checkbutton"
                    }
                }
            },
            "vtype": "bool",
            "name": "Virt-Bool-In1"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "checkbutton"
                    }
                }
            },
            "vtype": "bool",
            "name": "Virt-Bool-In2"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "checkbutton"
                    }
                }
            },
            "vtype": "bool",
            "name": "Virt-Bool-In3"
        },
        {
            "type": "signal",
            "uid": "signal3",
            "signals": {
                "value": {
                    "display": {
                        "section": "status",
                        "type": "led"
                    },
                    "net": "riov.Virt-Bool-In1.value"
                }
            },
            "dir": "output",
            "vtype": "bool",
            "name": "Virt-Bool-Out1"
        },
        {
            "type": "signal",
            "uid": "signal3",
            "signals": {
                "value": {
                    "display": {
                        "title": "Invert 1",
                        "section": "status",
                        "type": "led"
                    },
                    "net": "!riov.Virt-Bool-In1.value"
                }
            },
            "dir": "output",
            "vtype": "bool",
            "name": "Virt-Bool-Out2"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "title": "1 AND 2",
                        "section": "status",
                        "type": "led"
                    },
                    "net": "riov.Virt-Bool-In1.value AND riov.Virt-Bool-In2.value"
                }
            },
            "dir": "output",
            "vtype": "bool",
            "name": "Virt-Bool-Out3"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "title": "1 OR 2",
                        "section": "status",
                        "type": "led"
                    },
                    "net": "riov.Virt-Bool-In1.value OR riov.Virt-Bool-In2.value"
                }
            },
            "dir": "output",
            "vtype": "bool",
            "name": "Virt-Bool-Out4"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "title": "(1 AND 2) OR 3",
                        "section": "status",
                        "type": "led"
                    },
                    "net": "(riov.Virt-Bool-In1.value AND riov.Virt-Bool-In2.value) OR riov.Virt-Bool-In3.value"
                }
            },
            "dir": "output",
            "vtype": "bool",
            "name": "Virt-Bool-Out5"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "title": "Delayed",
                        "section": "status",
                        "estop": true,
                        "type": "led"
                    },
                    "net": "delay(riov.Virt-Bool-In1.value, 0.5, 1)"
                }
            },
            "dir": "output",
            "vtype": "bool",
            "name": "Virt-Bool-Delayed"
        },
        {
            "type": "signal",
            "signals": {
                "value": {
                    "display": {
                        "title": "Converted",
                        "section": "status"
                    },
                    "net": "conv(riov.Virt-Bool-In1.value, bit, float)"
                }
            },
            "dir": "output",
            "name": "Virt-Float-Conv"
        }
    ],
    "modules": [
        {
            "slot": "LEFT",
            "module": "china-bob5x"
        }
    ],
    "linuxcnc": {
        "simulation": true,
        "gui": "flexgui",
        "_gui": "axis",
        "_vcp_type": "pyvcp"
    }
}
