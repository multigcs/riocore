{
    "name": "XC7Z010",
    "description": "XC7Z010 - template - no PS - no Ethernet support",
    "boardcfg": "XC7Z010",
    "flashcmd": "openFPGALoader -c usb-blaster --index-chain 1 build/rio.bit",
    "plugins": [
        {
            "type": "blink",
            "pins": {
                "led": {
                    "pin": "W13"
                }
            },
            "frequency": 1.0,
            "uid": "blink0"
        },
        {
            "type": "_uart",
            "pins": {
                "rx": {
                    "pin": "F15"
                },
                "tx": {
                    "pin": "A16"
                }
            },
            "uid": "uart0"
        },
        {
            "type": "w5500",
            "pins": {
                "mosi": {
                    "pin": "R19"
                },
                "miso": {
                    "pin": "T20"
                },
                "sclk": {
                    "pin": "T19"
                },
                "sel": {
                    "pin": "U19"
                }
            },
            "uid": "w55000",
            "gw": "192.168.11.1",
            "ip": "192.168.11.194"
        },
        {
            "type": "pwmout",
            "pins": {
                "pwm": {
                    "pin": "W14"
                }
            },
            "uid": "pwmout0"
        },
        {
            "type": "bitin",
            "pins": {
                "bit": {
                    "pin": "V20"
                }
            },
            "uid": "bitin0"
        }
    ]
}
